{
  "name": "packrs",
  "version": "1.0.9",
  "description": "rsbuild for react",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "publish--patch": "./publish.sh patch",
    "publish--minor": "./publish.sh minor",
    "publish--major": "./publish.sh major"
  },
  "tags": [
    "react",
    "rspack",
    "rsbuild",
    "rust build"
  ],
  "keywords": [
    "react",
    "rsbuild",
    "rsbuild out-of-the-box dx for react project."
  ],
  "author": "leonwgc",
  "license": "MIT",
  "dependencies": {
    "@rsbuild/core": "^1.6.12",
    "@rsbuild/plugin-less": "^1.5.0",
    "@rsbuild/plugin-react": "^1.4.2",
    "@rsbuild/plugin-sass": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "files": [
    "lib"
  ]
}
